{
    "game": "SuperMarioBros-1-1-v1",
    "algos": ["A2C", "PPO", "DQN"],
    "train_states": ["Start.state"],
    "eval_state": ["Start.state", "Start.2P.state"],

    "agents_config": {
        "A2C": {
            "policy": "CnnPolicy",
            "verbose": 1,
            "tensorboard":"tensorboard_logs/mario/a2c",
            "save_path": "my_models/mario/mario_a2c/"
        },
        "PPO": {
            "policy": "CnnPolicy",
            "verbose": 1,
            "tensorboard":"tensorboard_logs/mario/ppo",
            "save_path": "my_models/mario/mario_ppo/"
        },
        "DQN": {
            "policy": "CnnPolicy",
            "verbose": 1,
            "tensorboard":"tensorboard_logs/mario/dqn",
            "save_path": "my_models/mario/mario_dqn/"
        }
    }
}